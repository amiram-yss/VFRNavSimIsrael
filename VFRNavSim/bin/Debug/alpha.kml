<Document>
  <name>A tour and some features</name>
  <open>1</open>

  <gx:Tour>
    <name>Current Aircraft Position</name>
    <gx:Playlist>
      <gx:FlyTo>
        <gx:duration>6.0</gx:duration>
        <Camera>
